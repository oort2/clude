<script>

let output=''

for(let i=1; i<5; i++){
	for(let j=0; j<i; j++){
		output += ' '
		  	}	
	for(let k=7; k>2*i; k--){
		output += '*'
	}	output +='\n'
}

for(let i=1; i<5; i++){
	for(let j=4; i<j; j--){
		output += ' '	
	}		
		for(let k=0; k<2*i; k++){
			output +='*' 
   }	output +='\n'
}

console.log(output)  

</script>
